<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment_inicio">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/RecyclerView"
        android:layout_width="match_parent"
        android:layout_height="580dp"
        android:layout_marginTop="60dp"
        tools:ignore="MissingClass" />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="57dp"
        android:background="#000000"
        android:backgroundTint="#000000"
        app:cardElevation="3dp"
        app:cardPreventCornerOverlap="false">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="188dp"
            android:layout_height="55dp"
            android:scaleType="fitCenter"
            android:src="@drawable/in" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_marginStart="340dp"
            android:layout_marginTop="17dp"
            android:onClick="mostrarmensaje"
            android:src="@drawable/instagramdirecto"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck,TouchTargetSizeCheck" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="45dp"
            android:layout_height="50dp"
            android:layout_marginStart="290dp"
            android:layout_marginTop="5dp"
            android:onClick="mostrarmas"
            android:src="@drawable/mass"
            tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck,TouchTargetSizeCheck,TouchTargetSizeCheck" />
    </androidx.cardview.widget.CardView>

</FrameLayout>